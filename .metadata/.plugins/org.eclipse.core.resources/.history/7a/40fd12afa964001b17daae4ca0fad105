		ArrayList<String> things = new ArrayList<String>(5);
		things.add("dog");
		things.add("lion");	
		things.add("rabbit");
		things.add("leopard");
		things.add(4, null);
		things.add("caribou");
		
		System.out.println(things);
		
		ArrayList<String>crackers = new ArrayList<>();
		
		crackers.add(null);
		int i = 1;
		for(String thing : things) {
			
			crackers.add(i, thing);
			++1;
		}
		
		System.out.println(things);
	}

}
